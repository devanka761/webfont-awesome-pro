{
  "name": "webfont-awesome-pro",
  "version": "1.1.1",
  "description": "(Webfont) Font Awesome Pro Downloader",
  "author": "Devanka761 <contact@devanka.id> (https://devanka.id/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devanka761/webfont-awesome-pro.git"
  },
  "bugs": {
    "url": "https://github.com/devanka761/webfont-awesome-pro/issues"
  },
  "homepage": "https://github.com/devanka761/webfont-awesome-pro#readme",
  "keywords": [
    "font-awesome-pro",
    "fontawesomepro",
    "font-awesome",
    "fontawesome",
    "font-awesome-pro-downloader",
    "font-awesome-downloader",
    "font",
    "webfont",
    "font-face"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/fapro.d.ts",
        "default": "./dist/module.mjs"
      },
      "require": {
        "types": "./dist/fapro.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles/*": "./dist/css/*",
    "./css/*": "./dist/css/*",
    "./scss/*": "./dist/scss/*"
  },
  "main": "dist/index.cjs",
  "module": "dist/module.mjs",
  "types": "dist/fapro.d.ts",
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=14"
  },
  "targets": {
    "main": {
      "source": "src/index.ts"
    },
    "module": {
      "source": "src/index.ts"
    },
    "types": {
      "source": "src/index.ts"
    },
    "cli": {
      "source": "src/webfapro.ts",
      "outputFormat": "esmodule"
    }
  },
  "scripts": {
    "type-check": "tsc -p . --pretty --noEmit",
    "lint": "eslint --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "check": "prettier --check .",
    "start": "tsx ./src/webfapro.ts",
    "clear": "rm -rf dist .parcel-cache",
    "create": "tsx ./src/create/start.ts",
    "build": "npm run format && npm run lint && npm run type-check && npm run clear && tsc --emitDeclarationOnly && parcel build && npm run create",
    "prepublishOnly": "npm run build",
    "getfont": "npx webfapro"
  },
  "bin": {
    "webfapro": "./dist/webfapro.js"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "nodejs-file-downloader": "^4.13.0",
    "postcss": "^8.5.5",
    "postcss-safe-parser": "^7.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@parcel/packager-ts": "^2.15.4",
    "@parcel/transformer-typescript-types": "^2.15.4",
    "@types/node": "^24.0.8",
    "@types/postcss-safe-parser": "^5.0.4",
    "parcel": "^2.15.4",
    "prettier": "^3.5.3",
    "tsx": "^4.20.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  }
}
